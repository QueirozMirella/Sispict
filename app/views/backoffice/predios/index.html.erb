<h1> Predios</h1>
<table  class="table table-bordered">
  <thead>
    <tr>
      <th>Nome</th>
      <th>Acronimo</th>
      <th>Laboratórios</th>
      <th>Cadastrar Laboratórios</th>
      <th colspan="3">Ações</th>
    </tr>
  </thead>

  <tbody>
    <% @predios.each do |predio| %>
      <tr>
        <td><%= predio.nome %></td>
        <td><%= predio.acronimo %></td>
        <td><%= link_to 'Visualizar' , backoffice_predio_laboratorios_path(predio) %></td>
        <td><%= link_to 'Cadastrar', new_backoffice_predio_laboratorio_path(predio) %></td>
        <td><%= link_to t('buttons.show'), [ :backoffice, predio ] %></td>
        <td><%= link_to t('buttons.edit'), edit_backoffice_predio_path(predio) %></td>
        <td><%= link_to t('buttons.destroy'), [ :backoffice, predio], method: :delete, data: { confirm: t('message.confirm') } %></td>
      </tr>
    <% end %>
  </tbody>
</table>